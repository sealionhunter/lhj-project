<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="model.Room" table="room">
		<id name="id">
			<generator class="assigned" />
		</id>
		<property generated="never" lazy="false" name="code" />
		<property generated="never" lazy="false" name="name" />
		<property generated="never" lazy="false" name="position" />
		<property generated="never" lazy="false" name="seats" />
		<property generated="never" lazy="false" name="remainSeats" />
	</class>
</hibernate-mapping>
