<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="model.Exam" table="exam">
		<id name="id">
			<generator class="identity" />
		</id>
		<property generated="never" lazy="false" name="name" />
        <property generated="never" lazy="false" name="applyBeginDate" />
		<property generated="never" lazy="false" name="applyDeadDate" />
		<property generated="never" lazy="false" name="examDate" />
		<property generated="never" lazy="false" name="examTime" />
		<property generated="never" lazy="false" name="examPosition" />
	</class>
</hibernate-mapping>
